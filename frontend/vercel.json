{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/test-auth.html",
      "dest": "/test-auth.html"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ]
}
