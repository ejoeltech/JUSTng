{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "max-age=31536000, immutable"
      }
    },
    {
      "src": "/(admin-users\\.html|create-demo-data\\.html|test-components\\.html)",
      "dest": "/$1"
    },
    {
      "src": "/vite\\.svg",
      "dest": "/vite.svg"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/dashboard",
      "dest": "/index.html"
    },
    {
      "src": "/map",
      "dest": "/index.html"
    },
    {
      "src": "/login",
      "dest": "/index.html"
    },
    {
      "src": "/register",
      "dest": "/index.html"
    },
    {
      "src": "/report",
      "dest": "/index.html"
    },
    {
      "src": "/admin",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
